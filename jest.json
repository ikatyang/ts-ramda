{
  "transform": {
    "/tests/.+\\.ts$": "dts-jest",
    "/snapshot-formatter\\.ts$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
  },
  "testRegex": "/tests/.+\\.ts$",
  "moduleFileExtensions": ["ts", "js"],
  "globals": {
    "_dts_jest_": {
      "type_detail": true,
      "snapshot_formatter": "src/utils/snapshot-formatter.ts"
    }
  }
}
